{
  "name": "console-server",
  "description": "Console Server",
  "version": "1.0.0",
  "author": {
    "name": "Mark Massoud",
    "email": "markmssd@gmail.com"
  },
  "engines": {
    "node": ">=6.x"
  },
  "scripts": {
    "start": "./node_modules/.bin/cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=dev PORT=4040 ./node_modules/.bin/nodemon server.js",
    "start:prod": "./node_modules/.bin/cross-env NODE_CONFIG_DIR=./config/env node server.js",
    "test": "./node_modules/.bin/cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors test/ --recursive -t 5000",
    "test:coverage": "./node_modules/.bin/cross-env NODE_CONFIG_DIR=./config/env NODE_ENV=test ./node_modules/.bin/istanbul cover -x config/express.js -x server.js -x crons/**/*.js _mocha -- --ui bdd --reporter spec --colors --compilers js:babel-core/register test/ --recursive -t 5000"
  },
  "dependencies": {
    "@google-cloud/pubsub": "0.14.5",
    "async": "~2.5.0",
    "aws-sdk": "~2.135.0",
    "bitbucket-server-nodejs": "~2.11.3",
    "bluebird": "~3.5.0",
    "body-parser": "1.17.0",
    "bodybuilder": "~2.2.6",
    "co": "~4.6.0",
    "compression": "~1.6.2",
    "config": "~1.26.2",
    "connect-flash": "~0.1.1",
    "connect-mongo": "~1.3.1",
    "cookie-parser": "~1.4.1",
    "cookie-session": "~1.2.0",
    "cors": "~2.8.3",
    "cross-env": "~3.2.4",
    "csurf": "1.9.0",
    "dotenv": "~4.0.0",
    "drone-node": "~1.0.1",
    "elasticsearch": "~13.3.1",
    "express": "4.15.0",
    "express-jwt": "~5.3.0",
    "express-session": "~1.15.1",
    "forever": "~0.15.1",
    "glob": "~7.1.2",
    "http-aws-es": "~3.1.3",
    "http-status": "~1.0.1",
    "js-base64": "~2.1.9",
    "js-yaml": "3.10.0",
    "json-stream": "~1.0.0",
    "jsonwebtoken": "~7.4.1",
    "kubernetes-client": "~3.11.0",
    "later": "~1.2.0",
